{
  "configurations": [
    {
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "name": "nodemon",
      "program": "${workspaceFolder}/src/index.js",
      "request": "launch",
      "restart": true,
      "runtimeExecutable": "node",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    }

  ]
}